{
  "name": "@cupist/expo-notification-kit",
  "version": "1.0.25",
  "description": "Cupist expo notification module.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "pnpm build:tsc && pnpm build:alias",
    "build:tsc": "pnpm tsc --outDir dist",
    "build:alias": "tsc-alias -p tsconfig.json",
    "prepublishOnly": "pnpm prebuild && pnpm build",
    "submit": "pnpm publish --registry=http://localhost:4873"
  },
  "keywords": [
    "notification",
    "cupist"
  ],
  "author": "cupist-terry",
  "license": "ISC",
  "dependencies": {
    "@cupist/notification-core": "^1.0.31",
    "expo-modules-core": "~1.11.14",
    "expo-notifications": "~0.27.8"
  },
  "peerDependencies": {
    "@cupist/notification-core": "workspace:*",
    "react": "18.2.0",
    "react-native": "0.73.2"
  }
}
