{
  "name": "@cupist/openapi-manager",
  "version": "1.0.49",
  "description": "Cupist OpenAPI Manager for React Native",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "types.d.ts",
    "README.md",
    "CHANGELOG.md"
  ],
  "sideEffects": [
    "dist/index.js",
    "dist/polyfill.js",
    "dist/glam.js"
  ],
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "pnpm build:tsc && pnpm build:alias",
    "build:tsc": "pnpm tsc --outDir dist --downlevelIteration",
    "build:alias": "tsc-alias -p tsconfig.json",
    "prepublishOnly": "pnpm prebuild && pnpm build",
    "submit": "pnpm publish --registry=http://localhost:4873",
    "generate:glam": "openapi-ts -f ./src/glam/openapi.config.ts",
    "generate:enfpy": "openapi-ts -f ./src/enfpy/openapi.config.ts",
    "generate:onlypass": "openapi-ts -f ./src/onlypass/openapi.config.ts"
  },
  "dependencies": {
    "@cupist/local-storage": "workspace:*",
    "@react-native-community/netinfo": "^11.1.0",
    "@tanstack/react-query": "^5.17.19",
    "axios": "^1.0.0",
    "react-native-device-info": "^11.1.0",
    "react-native-fetch-api": "^3.0.0",
    "react-native-polyfill-globals": "^3.1.0",
    "text-encoding": "^0.7.0",
    "url-parser": "^0.0.1",
    "web-streams-polyfill": "^3.3.3"
  },
  "peerDependencies": {
    "@cupist/local-storage": "^1.0.0",
    "@react-native-community/netinfo": "^11.1.0",
    "@tanstack/react-query": "^5.17.19",
    "axios": "^1.0.0",
    "react": "18.2.0",
    "react-native": "0.73.2",
    "react-native-device-info": "^11.1.0"
  },
  "devDependencies": {
    "@hey-api/openapi-ts": "0.87.1",
    "@tanstack/eslint-plugin-query": "^5.17.22",
    "@types/node": "^20.11.20"
  },
  "peerDevDependencies": {
    "@hey-api/openapi-ts": "0.87.1",
    "@tanstack/eslint-plugin-query": "^5.17.22",
    "@types/node": "^20.11.20"
  },
  "keywords": [],
  "author": "cupist-terry",
  "license": "ISC",
  "packageManager": "pnpm@10.20.0"
}
