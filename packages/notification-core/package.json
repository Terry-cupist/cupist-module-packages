{
  "name": "@cupist/notification-core",
  "version": "1.0.31",
  "description": "Cupist notification core module.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "pnpm build:tsc && pnpm build:alias",
    "build:tsc": "pnpm tsc --outDir dist",
    "build:alias": "tsc-alias -p tsconfig.json",
    "prepublishOnly": "pnpm prebuild && pnpm build",
    "submit": "pnpm publish --registry=http://localhost:4873"
  },
  "keywords": [
    "notification",
    "cupist"
  ],
  "peerDependencies": {
    "react": "18.2.0",
    "react-native": "0.73.2"
  },
  "author": "cupist-terry",
  "license": "ISC"
}
